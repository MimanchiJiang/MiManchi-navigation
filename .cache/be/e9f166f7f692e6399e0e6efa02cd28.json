\n    width: 160px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    padding: 20px 0;\r\n    background: #ffffff00;\r\n    border-radius: 4px;\r\n    color: #eee;\r\n\r\n}\r\n\r\n@media (max-width:500px) {\r\n    .siteList .addButton {\r\n        border: 1px solid #000;\r\n        width: 160px;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        flex-direction: column;\r\n        padding: 20px 0;\r\n        background: #ffffff00;\r\n        border-radius: 4px;\r\n        color: #000;\r\n\r\n\r\n    }\r\n\r\n}\r\n\r\n.siteList .site>.close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 5px;\r\n    display: none;\r\n    cursor: default;\r\n\r\n}\r\n\r\n.siteList .site:hover>.close {\r\n    display: block;\r\n}\r\n\r\n.siteList .addButton .icon {\r\n    width: 56px;\r\n    height: 56px;\r\n    vertical-align: -0.15em;\r\n    fill: currentColor;\r\n    overflow: hidden;\r\n}\r\n\r\n.siteList .addButton .text {\r\n    font-size: 14px;\r\n    margin-top: 4px;\r\n}\r\n\r\n.siteList .addButton .icon-wrapper {\r\n    width: 64px;\r\n    height: 64px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}"},"lineCount":null}},"error":null,"hash":"5d90545d47b84829a3feb1f782a9f801","cacheData":{"env":{}}}